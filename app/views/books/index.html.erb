<div class="main">
	<div class="lists">
		<h2 class="list-title">Books</h2>
		<table>
			<thead class="table-head">
				<tr>
					<th>title</th>
					<th>Body</th>
					<th colspan="3"></th>
				</tr>
			</thead>
			<tbody>
				<% @books.each do |book| %>
				<tr>
					<td><%= book.title %></td>
					<td><%= book.body %></td>
					<td><a href="#">show</a></td>
					<td><a href="#">update</a></td>
					<td><a href="#">delete</a></td>
				</tr>
				<% end %>
			</tbody>
		</table>
	</div>
</div>